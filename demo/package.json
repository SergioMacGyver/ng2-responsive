{
  "name": "ng2-responsive-webpack-aot",
  "version": "0.1.0",
  "description": "Starter application of NG2-RESPONSIVE on AOT with Express and Webpack",
  "engines": {
    "node": ">= 5.4.1"
  },
  "scripts": {
    "prestart": "npm install",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "start": "npm run prestart && npm run ngc && npm run webpack && webpack-dev-server --inline --progress --port 8080",
    "start-prod": "rimraf public && npm run ngc && npm run webpack-prod && node server.js",
    "webpack": "webpack --config ./config/webpack.dev.js",
    "webpack-aot": "webpack --config ./config/webpack.aot.config.js",
    "webpack-prod": "webpack --config ./config/webpack.prod.config.js",
    "test": "karma start"
  },
