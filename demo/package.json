{
  "name": "ng2-responsive-webpack-aot",
  "version": "1.1.0",
  "description": "Starter application of NG2-RESPONSIVE on AOT with Express and Webpack",
  "engines": {
    "node": ">= 5.4.1"
  },
  "scripts": {
    "ngc": "node_modules/.bin/ngc -p tsconfig.aot.json --locale=es-ES",
    "tsc": "tsc",
    "rollup": "node_modules/.bin/rollup -c rollup-config.js",
    "prod:server": "node server.js",
    "dev:server": "live-server  --mount=ROUTE:/public/aot --entry-file=/index.aot.html",
    "start": "npm run prod:server",
    "webpack:aot": "webpack --config webpack-aot.js",
    "webpack:jit": "cd src && npm run tsc && cd .. && webpack --config webpack-jit.js",
    "build": "npm run ngc && npm run webpack:jit && npm run webpack:aot && npm run rollup",
    "map:explorer": "node_modules/.bin/source-map-explorer aot/dist/build.js",
    "brotli:compress": "bro --force --quality 10 --input bundle.min.js --output bundle.js.brotli"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/core": "2.1.2",
